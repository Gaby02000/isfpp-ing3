{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend Flask API",
  "private": true,
  "scripts": {
    "dev": "python app.py",
    "start": "python app.py",
    "install:python": "pip3 install -r requirements.txt",
    "seed": "python -m seed.insert_seed",
    "seed:clean": "python -m seed.insert_seed --clean",
    "db:init": "flask db init",
    "db:migrate": "flask db migrate -m",
    "db:upgrade": "flask db upgrade",
    "db:downgrade": "flask db downgrade",
    "db:history": "flask db history",
    "db:current": "flask db current",
    "db:reset": "flask db downgrade base && flask db upgrade head",
    "db:revision": "flask db revision --autogenerate -m"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "globals": "^16.5.0"
  }
}
